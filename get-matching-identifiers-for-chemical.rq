#+ summary: Get alternative identifiers for a specific chemical (example uses CAS 107-18-6).
#+ endpoint_in_url: False

SELECT ?CAS ?ChemicalName ?MatchingIDs
WHERE {
 ?chemical a cheminf:CHEMINF_000000 ;
 dc:title ?ChemicalName ;
 cheminf:CHEMINF_000446 ?CAS ;
 skos:exactMatch ?MatchingIDs . 
 VALUES ?chemical {casrn:107-18-6}
}
